<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.controller.ResidentListController">
    <VBox>
      <TableView fx:id="residentTableView">
        <columns>
          <TableColumn fx:id="firstNameTableColumn" prefWidth="113.0" text="First Name" />
          <TableColumn fx:id="lastNameTableColumn" prefWidth="126.5" text="Last Name" />
            <TableColumn fx:id="apartmentTableColumn" prefWidth="127.0" text="Apartment" />
            <TableColumn fx:id="phoneNumberTableColumn" prefWidth="145.0" text="Phone Number" />
            <TableColumn fx:id="emailTableColumn" minWidth="0.0" prefWidth="147.0" text="Email" />
            <TableColumn fx:id="nationalIDTableColumn" maxWidth="215.5" minWidth="-1.0" prefWidth="215.5" text="National ID" />
        </columns>
      </TableView>
         <Button fx:id="newResidentButton" mnemonicParsing="false" onAction="#newResidentButtonOnAction" text="New" />
    </VBox>
</AnchorPane>
