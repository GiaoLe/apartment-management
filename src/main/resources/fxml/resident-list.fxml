<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21"
            fx:controller="com.example.demo.ResidentListController">
    <VBox prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <ListView fx:id="residentListView" prefHeight="400.0" prefWidth="600.0"/>
        <HBox>
            <Button fx:id="menuButton" mnemonicParsing="false" onAction="#menuButtonOnAction" text="Menu"/>
            <Button fx:id="newResidentButton" mnemonicParsing="false" onAction="#newResidentButtonOnAction"
                    text="New"/>
        </HBox>
    </VBox>
</AnchorPane>
