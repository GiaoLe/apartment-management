<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.CollectionListController">
   <children>
      <VBox>
         <children>
            <ListView fx:id="collectionListView" prefHeight="209.0" prefWidth="533.0" />
            <HBox>
               <children>
                  <Button fx:id="menuButton" mnemonicParsing="false" onAction="#menuButtonOnAction" text="Menu" />
                  <Button fx:id="newButton" mnemonicParsing="false" onAction="#newButtonOnAction" text="New" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
