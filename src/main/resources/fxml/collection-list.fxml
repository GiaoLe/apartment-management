<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.controller.CollectionListController">
   <VBox prefHeight="309.0" prefWidth="413.0">
      <TableView fx:id="collectionsTableView" prefHeight="400.0" prefWidth="413.0">
        <columns>
          <TableColumn fx:id="nameTableColumn" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="typeTableColumn" prefWidth="85.5" text="Type" />
            <TableColumn fx:id="amountTableColumn" prefWidth="91.5" text="Amount" />
            <TableColumn fx:id="descriptionTableColumn" prefWidth="160.0" text="Description" />
        </columns>
      </TableView>
      <Button fx:id="newButton" mnemonicParsing="false" onAction="#newButtonOnAction" text="New" />
   </VBox>
</AnchorPane>
