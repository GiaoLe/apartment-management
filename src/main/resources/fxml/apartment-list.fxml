<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.demo.controller.ApartmentListController">
   <BorderPane prefHeight="200.0" prefWidth="542.0">
      <center>
         <ListView fx:id="apartmentListView" prefHeight="175.0" prefWidth="382.0" BorderPane.alignment="CENTER"/>
      </center>
      <bottom>
         <HBox BorderPane.alignment="CENTER">
            <Button fx:id="menuButton" mnemonicParsing="false" onAction="#menuButtonOnAction" text="Menu"/>
            <Button fx:id="newButton" mnemonicParsing="false" onAction="#newButtonOnAction" text="New"/>
         </HBox>
      </bottom>
   </BorderPane>
</AnchorPane>
