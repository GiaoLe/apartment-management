<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.MenuController">
   <children>
      <HBox>
         <children>
            <Button fx:id="residentListButton" mnemonicParsing="false" onAction="#residentListButtonOnAction" prefHeight="25.0" prefWidth="95.0" text="Residents" />
            <Button fx:id="apartmentListButton" mnemonicParsing="false" onAction="#apartmentListButtonOnAction" text="Apartments" />
            <MenuButton mnemonicParsing="false" text="Finance">
              <items>
                <MenuItem fx:id="createCollectionMenuItem" mnemonicParsing="false" onAction="#createCollectionMenuItemOnAction" text="Create collection" />
                <MenuItem fx:id="reportsMenuItem" mnemonicParsing="false" onAction="#reportsMenuItemOnAction" text="Reports" />
              </items>
            </MenuButton>
         </children>
      </HBox>
   </children>
</AnchorPane>
