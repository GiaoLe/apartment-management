<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.demo.MenuController">
   <HBox>
      <Button fx:id="residentListButton" mnemonicParsing="false" onAction="#residentListButtonOnAction"
              prefHeight="25.0" prefWidth="95.0" text="Residents"/>
      <Button fx:id="apartmentListButton" mnemonicParsing="false" onAction="#apartmentListButtonOnAction"
              text="Apartments"/>
      <Button fx:id="collectionButton" mnemonicParsing="false" onAction="#collectionButtonOnAction"
              text="Collection"/>
   </HBox>
</AnchorPane>
