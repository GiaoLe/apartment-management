<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.controller.ApartmentFormController">
   <VBox prefHeight="217.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <Label text="Create a new apartment" />
      <HBox>
         <Label layoutX="10.0" layoutY="10.0" text="number" />
         <TextField fx:id="numberTextField" layoutX="42.0" layoutY="10.0" />
      </HBox>
      <HBox layoutX="10.0" layoutY="27.0">
         <Label text="Area" />
         <TextField fx:id="areaTextField" />
      </HBox>
      <HBox layoutX="10.0" layoutY="52.0">
         <children>
            <Label text="id" />
            <TextField fx:id="idTextField" />
         </children>
      </HBox>
      <HBox prefHeight="26.0" prefWidth="200.0">
         <children>
            <Label text="Type" />
            <TextField fx:id="typeTextField" />
         </children>
      </HBox>
      <HBox prefHeight="32.0" prefWidth="200.0">
         <children>
            <HBox prefHeight="26.0" prefWidth="200.0">
               <children>
                  <Label text="status" />
                  <TextField fx:id="statusTextField" />
               </children>
            </HBox>
         </children>
      </HBox>
      <Button fx:id="submitButton" mnemonicParsing="false" onAction="#submitButtonOnAction" text="Submit" />
      <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleBackAction" text="Back" />
   </VBox>
</AnchorPane>
